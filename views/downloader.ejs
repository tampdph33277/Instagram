<main class="mb-5" style="background: #4a4a4a">



    <h1 class="text-black text-center fw-bold"><%= __('h1') %></h1>
    <h2 class="text-black text-center mt-4"><%= __('h2') %></h2>
    <div class="row">
        <% if (mediaData && mediaData.length > 0) { %>
            <% mediaData.forEach(function(media) { %>
                <div id="card_download" class="card col-12 col-sm-6 col-md-4">
                    <% if (media.pictureUrls && media.pictureUrls.length > 0) { %>

                        <% media.pictureUrls.forEach(function(pictureUrl) { %>
                            <img
                                    crossorigin="anonymous"
                                    id="thumbnail"
                                    src="https://ig.instasaver.me/?url=<%= pictureUrl %>"
                                    class="card-img-top"
                                    alt="img...">

                            <p>

                                <a href="<%= pictureUrl %>" download class="btn btn-primary btn-sm">Download</a>
                            </p>
                        <% }); %>
                    <% } %>
                    <% if (media.videoUrls && media.videoUrls.length > 0) { %>
                        <h2>Videos</h2>
                        <% media.videoUrls.forEach(function(videoUrl) { %>
                            <% if (media.picture) { %>
                                <img
                                        crossorigin="anonymous"
                                        id="thumbnail"
                                        src="https://ig.instasaver.me/?url=<%= media.picture %>"
                                        class="card-img-top"
                                        alt="img...">
                            <% } %>
                            <p>

                                <a href="<%= videoUrl %>" download class="btn btn-primary btn-sm">Download</a>
                            </p>
                        <% }); %>
                    <% } %>
                </div>
            <% }); %>
        <% } else { %>
            <div class="col-12">
                <p>No media links found</p>
            </div>
        <% } %>
    </div>

