<main class="mb-5" style="background: #fff">
    <div class="dow" style=" padding-bottom:5%; background: linear-gradient(to right, #f44336, #e91e63, #9c27b0, #673ab7, #3f51b5);">
        <h1 class=" text-white text-center fw-bold display-1 "><%= __('h1') %></h1>
        <h2 class=" text-white text-center fw-medium display-4 "><%= __('h2') %></h2>
        <div class="row align-content-center justify-content-center" style=" padding-left:5%;padding-right:5%;gap: 20px">
            <% if (mediaData && mediaData.length > 0) { %>

                <!-- Iterate through mediaData for regular pictures and videos -->
                <% mediaData.forEach(function(media) { %>
                    <% if ((media.pictureUrls && media.pictureUrls.length > 0) || media.picture || (media.videoUrls && media.videoUrls.length > 0)) { %>
                        <div id="card_download" class="card col-12 col-sm-6 col-md-4 pt-2">
                            <% let hasVideo = media.videoUrls && media.videoUrls.length > 0; %>
                            <% if (media.pictureUrls && media.pictureUrls.length > 0) { %>
                                <% media.pictureUrls.forEach(function(pictureUrl) { %>
                                    <img
                                            crossorigin="anonymous"
                                            id="thumbnail"
                                            src="https://ig.instasaver.me/?url=<%= pictureUrl %>"
                                            class="card-img-top"
                                            alt="img...">
                                    <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                        <% if (hasVideo) { %>
                                            <!-- Nếu có video, chỉ hiển thị nút "Download Video" -->
                                            <% media.videoUrls.forEach(function(videoUrl) { %>
                                                <a href="<%= videoUrl %>" download class="btn btn-primary btn-sm text-black fw-bold">Download Video</a>
                                            <% }); %>
                                        <% } else { %>
                                            <!-- Nếu không có video nhưng có ảnh, hiển thị nút "Download Picture" -->
                                            <a href="<%= pictureUrl %>" download class="btn btn-primary btn-sm p-2 text-black fw-bold">Download Picture</a>
                                        <% } %>
                                    </p>
                                <% }); %>
                            <% } else if (media.picture) { %>
                                <img
                                        crossorigin="anonymous"
                                        id="thumbnail"
                                        src="https://ig.instasaver.me/?url=<%= media.picture %>"
                                        class="card-img-top"
                                        alt="img...">
                                <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                    <% if (hasVideo) { %>
                                        <!-- Nếu có video, chỉ hiển thị nút "Download Video" -->
                                        <% media.videoUrls.forEach(function(videoUrl) { %>
                                            <a href="<%= videoUrl %>" download class="btn btn-primary btn-sm text-black fw-bold ">Download Video</a>
                                        <% }); %>
                                    <% } else { %>
                                        <!-- Nếu không có video nhưng có ảnh, hiển thị nút "Download Picture" -->
                                        <a href="<%= media.picture %>" download class="btn btn-primary btn-sm p-2 text-black fw-bold">Download Picture</a>
                                    <% } %>
                                </p>
                            <% } %>
                        </div>
                    <% } %>
                <% }); %>

                <!-- Iterate through mediaData for story pictures -->
                <% mediaData.forEach(function(media) { %>
                    <% if (media.picture_stos && media.picture_stos.length > 0) { %>
                        <div id="card_download" class="card col-12 col-sm-6 col-md-4 pt-2">
                            <img
                                    crossorigin="anonymous"
                                    id="thumbnail"
                                    src="https://ig.instasaver.me/?url=<%= media.picture_stos[0] %>"
                                    class="card-img-top"
                                    alt="Story thumbnail">
                            <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                <a href="https://photo.instasaver.me/?url=<%= media.picture_stos[0] %>" download class="btn btn-primary btn-sm text-black fw-bold">Download Story Picture</a>
                            </p>
                        </div>
                    <% } %>
                <% }); %>

                <!-- Iterate through mediaData for story videos -->
                <% mediaData.forEach(function(media) { %>
                    <% if (media.video_stos && media.video_stos.length > 0) { %>
                        <div id="card_download" class="card col-12 col-sm-6 col-md-4 pt-2">
                            <img
                                    crossorigin="anonymous"
                                    id="thumbnail"
                                    src="https://ig.instasaver.me/?url=<%= media.picture_stos[0] %>"
                                    class="card-img-top"
                                    alt="Story thumbnail">
                            <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                <a href="<%= media.video_stos[0] %>" download class="btn btn-primary btn-sm text-black fw-bold">Download Story Video</a>
                            </p>
                        </div>
                    <% } %>
                <% }); %>

            <% } else { %>
                <div class="col-12">
                    <p>No media links found</p>
                </div>
            <% } %>
        </div>

    </div >



</main>
<script >
    document.addEventListener("DOMContentLoaded", function() {
        const navbarToggler = document.querySelector(".navbar-toggler");
        const navbarNav2 = document.querySelector("#navbarNav2");
        const header = document.querySelector("header");

        navbarToggler.addEventListener("click", function() {
            navbarNav2.classList.toggle("show");
            header.classList.toggle("expanded");
        });
    });

</script>

