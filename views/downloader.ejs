    <div class="row align-content-center justify-content-center" style="gap: 20px">
        <% if (mediaData && mediaData.length > 0) { %>

            <!-- Iterate through mediaData for regular pictures and videos -->
            <% mediaData.forEach(function(media) { %>
                <% if ((media.pictureUrls && media.pictureUrls.length > 0) || (media.videoUrls && media.videoUrls.length > 0)) { %>
                    <div id="card_download" class="card col-12 col-sm-6 col-md-4 pt-2">
                        <% if (media.pictureUrls && media.pictureUrls.length > 0) { %>
                            <% media.pictureUrls.forEach(function(pictureUrl) { %>
                                <img
                                        crossorigin="anonymous"
                                        id="thumbnail"
                                        src="https://ig.instasaver.me/?url=<%= pictureUrl %>"
                                        class="card-img-top"
                                        alt="img...">
                                <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                    <a href="<%= pictureUrl %>" download class="btn btn-primary btn-sm p-2 fs-4">Download Picture</a>
                                </p>
                            <% }); %>
                        <% } %>
                        <% if (media.videoUrls && media.videoUrls.length > 0) { %>
                            <% media.videoUrls.forEach(function(videoUrl) { %>
                                <% if (media.picture) { %>
                                    <img
                                            crossorigin="anonymous"
                                            id="thumbnail"
                                            src="https://ig.instasaver.me/?url=<%= media.picture %>"
                                            class="card-img-top"
                                            alt="img...">
                                <% } %>
                                <p class="d-flex mt-3 w-100 align-content-center justify-content-center">
                                    <a href="<%= videoUrl %>" download class="btn btn-primary btn-sm fs-4">Download Video</a>
                                </p>
                            <% }); %>
                        <% } %>
                    </div>
                <% } %>
            <% }); %>

            <!-- Iterate through mediaData for story pictures -->
            <% mediaData.forEach(function(media) { %>
                <% if (media.picture_stos && media.picture_stos.length > 0) { %>
                    <div id="card_download" class="card col-12 col-sm-6 col-md-4">
                        <img
                                crossorigin="anonymous"
                                id="thumbnail"
                                src="https://ig.instasaver.me/?url=<%= media.picture_stos[0] %>"
                                class="card-img-top"
                                alt="Story thumbnail">
                        <div class="card-body d-flex mt-3 w-100 align-content-center justify-content-center">
                            <a href="https://photo.instasaver.me/?url=<%= media.picture_stos[0] %>" download class="btn btn-primary btn-sm fs-4">Download Story Picture</a>
                        </div>
                    </div>
                <% } %>
            <% }); %>

            <!-- Iterate through mediaData for story videos -->
            <% mediaData.forEach(function(media) { %>
                <% if (media.video_stos && media.video_stos.length > 0) { %>
                    <div id="card_download" class="card col-12 col-sm-6 col-md-4">
                        <video controls class="card-img-top">
                            <source src="<%= media.video_stos[0] %>" type="video/mp4">
                            Your browser does not support the video tag.
                        </video>
                        <div class="card-body d-flex mt-3 w-100 align-content-center justify-content-center">
                            <a href="<%= media.video_stos[0] %>" download class="btn btn-primary btn-sm fs-4">Download Story Video</a>
                        </div>
                    </div>
                <% } %>
            <% }); %>

        <% } else { %>
            <div class="col-12">
                <p>No media links found</p>
            </div>
        <% } %>
    </div>

